{

  "manifest_version": 2,
  "name": "leaflike",
  "version": "1.0",

    "description": "Extension for leaflike bookmarking tool",

  "icons": {
    "48": "icons/border-48.png"
  },
    "permissions": [
	"activeTab",
	"tabs"
    ],

    "browser_action": {
	"default_icon": "icons/border-48.png",
	"default_title": "leaflike",
	"default_popup": "popup/add_bookmark.html"
  }

}
